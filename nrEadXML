rule "Copy identifier"
when
exist "//ead/eadheader/eadid"
then
copy "//ead/eadheader/eadid" to "dc"."identifier"
end

rule "Copy link to resource"
when
exist "/ead/eadheader/eadid/@url"
then
copy "/ead/eadheader/eadid/@url" to "dc"."identifier"("dcterms:URI")
end

rule "copy url for full-text indexing (URL for Full-text Indexing)"
when
exist "/ead/eadheader/eadid/@url"
then
copy "/ead/eadheader/eadid/@url" to "discovery"."local13"
end

rule "copy description7"
when
exist "//ead/archdesc/dsc/c[7]"
then
copy all "concat(//ead/archdesc/dsc/c[7]/did/unittitle,': Box: ',//ead/archdesc/dsc/c[7]/did/container[@type='box'],' Folder: ',//ead/archdesc/dsc/c[7]/did/container[@type='folder'])"("; ") to "dc"."description"
end

